<div class="container">
  <div class="row">
    <div class="col">

<h2>User info</h2>
  <%= render 'users/info',user: @user %>

<h2 class=>New book</h2>
  <%= render 'books/new',book: @book %>
  </div>
  <div class="col">


<h2>Users</h2>
      <table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= attachment_image_tag @book.user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %></td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>

</div>
</div>